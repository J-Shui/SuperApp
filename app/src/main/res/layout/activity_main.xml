<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".MainActivity">
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:contentDescription="这是个设么"
                android:src="@mipmap/ic_launcher"
                />
        <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="btnSwitchLandscape"
                android:text="切换横屏"
                />
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="添加历史"
            android:onClick="btnAddHistory"
            />
        <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="btnJumpAndroid5"
                android:text="跳到Android 5.0"
                />
    <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="btnJumpPalette"
            android:text="跳到Palette"
            />
         <fragment
            android:id="@+id/fragment_history"
            class="com.jinshui.superapp.fragment.HistoryFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
</LinearLayout>
